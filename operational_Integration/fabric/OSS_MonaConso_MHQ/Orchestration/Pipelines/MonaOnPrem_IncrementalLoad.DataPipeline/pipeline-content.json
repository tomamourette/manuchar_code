              "typeProperties": {
                "scripts": [
                  {
                    "text": {
                      "value": "UPDATE stg\nSET stg.ODSRecordStatus = 'N'\nFROM \n@{pipeline().parameters.DestinationSchema}.@{pipeline().parameters.TableName}__STG stg\nLEFT JOIN \n@{pipeline().parameters.DestinationSchema}.@{pipeline().parameters.TableName}__ODS ods\nON stg.ODSKey = ods.ODSKey \nWHERE ods.ODSKey IS NULL;",
                      "type": "Expression"
